<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="Settings" type="HareDu.Tests, Config" />
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="HareDu.Tests.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
        <sectionGroup name="common">
            <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
        </sectionGroup>
    </configSections>
    <common>
        <logging>
            <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1213">
                <arg key="configType" value="FILE" />
                <arg key="configFile" value="~/log4net.config" />
            </factoryAdapter>
        </logging>
    </common>
    <userSettings>
        <HareDu.Tests.Settings>
            <setting name="HostUrl" serializeAs="String">
                <value>http://localhost:15672</value>
            </setting>
            <setting name="LoginUsername" serializeAs="String">
                <value>guest</value>
            </setting>
            <setting name="LoginPassword" serializeAs="String">
                <value>guest</value>
            </setting>
            <setting name="VirtualHost" serializeAs="String">
                <value>MyTestVirtualHost</value>
            </setting>
            <setting name="Exchange" serializeAs="String">
                <value>MyTestExchange</value>
            </setting>
            <setting name="Queue" serializeAs="String">
                <value>MyTestQueue</value>
            </setting>
            <setting name="Username" serializeAs="String">
                <value>test_user</value>
            </setting>
            <setting name="UserPassword" serializeAs="String">
                <value>test_user_password</value>
            </setting>
            <setting name="UserPermissionsTags" serializeAs="String">
                <value>administrator</value>
            </setting>
            <setting name="RoutingKey" serializeAs="String">
                <value>test_routing_key</value>
            </setting>
            <setting name="Policy" serializeAs="String">
                <value>MyTestPolicy</value>
            </setting>
            <setting name="Component" serializeAs="String">
                <value>MyTestComponent</value>
            </setting>
            <setting name="Parameter" serializeAs="String">
                <value>MyTestParameter</value>
            </setting>
            <setting name="LoggerName" serializeAs="String">
                <value>HareDuLogger</value>
            </setting>
        </HareDu.Tests.Settings>
    </userSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="log4net" publicKeyToken="669e0ddf0bb1aa2a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.8.0" newVersion="2.0.8.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>